{{#each dichotomy}}
<section>
	<h2>{{title}}</h2>
	<ol id="{{type}}">
		{{#each choice}}
		<li>
			<label><input type="radio" name="{{../type}}{{@index}}" value="true" required><span></span>{{A}}</label>
			<label><input type="radio" name="{{../type}}{{@index}}" value="false"><span></span>{{B}}</label>
		</li>
		{{/each}}
	</ol>
</section>
{{/each}}
