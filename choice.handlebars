{{#each latitude}}
<section>
	<h2>{{title}}</h2>
	<ol id="{{type}}">
		{{#each choice}}
		<li>
			<label><input type="radio" name="{{../type}}{{@index}}" value="true">{{A}}</label>
			<label><input type="radio" name="{{../type}}{{@index}}" value="false">{{B}}</label>
		</li>
		{{/each}}
	</ol>
</section>
{{/each}}
